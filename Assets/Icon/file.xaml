<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Viewbox x:Key="FileIcon" Width="16" Height="16">
        <Canvas Width="64" Height="64">
            <!-- 파일 본체 -->
            <Path Fill="#F7F7F7" Stroke="#444444" StrokeThickness="2">
                <Path.Data>
                    <PathGeometry>
                        <PathFigure StartPoint="14,8">
                            <LineSegment Point="38,8"/>
                            <LineSegment Point="50,20"/>
                            <LineSegment Point="50,56"/>
                            <LineSegment Point="14,56"/>
                            <LineSegment Point="14,8"/>
                        </PathFigure>
                    </PathGeometry>
                </Path.Data>
            </Path>
            <!-- 파일 접힌 부분 -->
            <Path Fill="#E0E0E0" Stroke="#444444" StrokeThickness="2">
                <Path.Data>
                    <PathGeometry>
                        <PathFigure StartPoint="38,8">
                            <LineSegment Point="38,20"/>
                            <LineSegment Point="50,20"/>
                            <LineSegment Point="38,8"/>
                        </PathFigure>
                    </PathGeometry>
                </Path.Data>
            </Path>
            <!-- 텍스트 라인 -->
            <Line X1="22" Y1="30" X2="42" Y2="30" Stroke="#5C95E8" StrokeThickness="2"/>
            <Line X1="22" Y1="38" X2="42" Y2="38" Stroke="#5C95E8" StrokeThickness="2"/>
            <Line X1="22" Y1="46" X2="34" Y2="46" Stroke="#5C95E8" StrokeThickness="2"/>
        </Canvas>
    </Viewbox>
</ResourceDictionary>
